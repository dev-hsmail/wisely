<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wisely.mapper.AdminMapper">

	<select id="adminOk" parameterType="com.wisely.domain.AdminVO" resultType="com.wisely.domain.AdminVO">
